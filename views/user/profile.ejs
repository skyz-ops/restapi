<body>

    <div>
        <%- include('../layouts/msg') %>
            <%- include('../layouts/errors') %>
    </div>

    <div class="card">
        <div class="card-header">
            Profil Kamu
        </div>
        <div class="card-body">
            <blockquote class="blockquote mb-0">
                <p class="card-text">Email: <%- email %>
                </p>
                <p class="card-text">Username: <%- username %>
                </p>
                <p class="card-text">Apikey: <%- apikey %>
                </p>
                <p class="card-text">Limit: <%- limit %>
                </p>
                <p class="card-text">Premium: <%- isPrem %>
                </p>
                <% if (premium) { %>
                    <p class="card-text">Kedaluarsa: <%- expired %>
                    </p>
                    <% } %>
                        <a href="#" class="btn btn-primary" for="snk" data-bs-toggle="modal"
                            data-bs-target="#snk">Syarat
                            dan Ketentuan</a><br><br>
                        <a href="#" class="btn btn-primary" for="changePassword" data-bs-toggle="modal"
                            data-bs-target="#changePassword">Ganti Password</a><br><br>
                        <a href="#" class="btn btn-primary" for="changeKey" data-bs-toggle="modal"
                            data-bs-target="#changeKey">Ganti Apikey</a>
            </blockquote>
        </div>
    </div>

    <%- include('../layouts/modal') %>